Function Get_SELang
{
	param (
		$Name,
		$Newlang,
		$Edition,
		$Replace
	)

	$NewDetailed = @()
	ForEach ($item in $Global:SELang) {
		if ($item.Region -eq $Newlang) {
			ForEach ($NewEdition in $item.Language) {
				if ($NewEdition.Edition -eq $Edition) {
					$ReplaceImageName = $($NewEdition.ImageName -f $Replace)

					if ([string]::IsNullOrEmpty($NewEdition.Description)) {
						$ReplaceDescription = $ReplaceImageName
					} else {
						$ReplaceDescription = $($NewEdition.Description -f $Replace)
					}

					if ([string]::IsNullOrEmpty($NewEdition.DisplayName)) {
						$ReplaceDisplayName = $ReplaceImageName
					} else {
						$ReplaceDisplayName = $($NewEdition.DisplayName -f $Replace)
					}

					if ([string]::IsNullOrEmpty($NewEdition.DisplayDescription)) {
						$ReplaceDisplayDescription = $ReplaceImageName
					} else {
						$ReplaceDisplayDescription = $($NewEdition.DisplayDescription -f $Replace)
					}

					$NewDetailed = @{
						ImageName = $ReplaceImageName
						Description        = $ReplaceDescription
						DisplayName        = $ReplaceDisplayName
						DisplayDescription = $ReplaceDisplayDescription
					}
				}
			}

			if ($NewDetailed.count -gt 0) {
			} else {
				$NewDetailed = @{
					ImageName = $Name
					Description        = $Name
					DisplayName        = $Name
					DisplayDescription = $Name
				}
			}

			$Customlist = [PSCustomObject]@{
				Name        = $Name
				Region      = $Newlang
				Language    = $NewDetailed
			}

			return $Customlist
		}
	}

	ForEach ($item in $Global:SELang) {
		if ($item.Region -eq "en-US") {
			ForEach ($NewEdition in $item.Language) {
				if ($NewEdition.Edition -eq $Edition) {
					$ReplaceImageName = $($NewEdition.ImageName -f $Replace)

					if ([string]::IsNullOrEmpty($NewEdition.Description)) {
						$ReplaceDescription = $ReplaceImageName
					} else {
						$ReplaceDescription = $($NewEdition.Description -f $Replace)
					}

					if ([string]::IsNullOrEmpty($NewEdition.DisplayName)) {
						$ReplaceDisplayName = $ReplaceImageName
					} else {
						$ReplaceDisplayName = $($NewEdition.DisplayName -f $Replace)
					}

					if ([string]::IsNullOrEmpty($NewEdition.DisplayDescription)) {
						$ReplaceDisplayDescription = $ReplaceImageName
					} else {
						$ReplaceDisplayDescription = $($NewEdition.DisplayDescription -f $Replace)
					}

					$NewDetailed = @{
						ImageName = $ReplaceImageName
						Description        = $ReplaceDescription
						DisplayName        = $ReplaceDisplayName
						DisplayDescription = $ReplaceDisplayDescription
					}
				}
			}

			if ($NewDetailed.count -gt 0) {
			} else {
				$NewDetailed = @{
					ImageName = $Name
					Description        = $Name
					DisplayName        = $Name
					DisplayDescription = $Name
				}
			}

			$Customlist = [PSCustomObject]@{
				Name        = $Name
				Region      = $Newlang
				Language    = $NewDetailed
			}

			return $Customlist
		}
	}
}